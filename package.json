{
  "name": "@utexo/rgb-sdk",
  "version": "1.0.0",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^22.17.2",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "type": "commonjs",
  "scripts": {
    "dev": "tsc -w",
    "build": "tsup",
    "test": "npm run build && NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "npm run build && NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:coverage": "npm run build && NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "version:patch": "npm version patch && git push && git push --tags && npm publish --access public",
    "version:minor": "npm version minor && git push && git push --tags && npm publish --access public",
    "version:major": "npm version major && git push && git push --tags && npm publish --access public",
    "example:complete": "node --experimental-wasm-modules example-flow.mjs",
    "generate_keys": "node utexo_test/generate_keys.mjs",
    "address": "node utexo_test/address.mjs",
    "btcbalance": "node utexo_test/btcbalance.mjs",
    "createutxos": "node utexo_test/createutxos.mjs",
    "listassets": "node utexo_test/listassets.mjs",
    "blindreceive": "node utexo_test/blindreceive.mjs",
    "witnessreceive": "node utexo_test/witnessreceive.mjs",
    "onchainreceive": "node utexo_test/onchainreceive.mjs",
    "decodergbinvoice": "node utexo_test/decodergbinvoice.mjs",
    "refresh": "node utexo_test/refresh.mjs",
    "listtransfers": "node utexo_test/listtransfers.mjs",
    "onchainsendbegin": "node utexo_test/onchainsendbegin.mjs",
    "signpsbt": "node utexo_test/signpsbt.mjs",
    "onchainsendend": "node utexo_test/onchainsendend.mjs",
    "onchainsend": "node utexo_test/onchainsend.mjs",
    "listunspents": "node utexo_test/listunspents.mjs",
    "getonchainsendstatus": "node utexo_test/getonchainsendstatus.mjs",
    "createlightninginvoice": "node utexo_test/createlightninginvoice.mjs",
    "paylightninginvoicebegin": "node utexo_test/paylightninginvoicebegin.mjs",
    "paylightninginvoice": "node utexo_test/paylightninginvoice.mjs",
    "paylightninginvoiceend": "node utexo_test/paylightninginvoiceend.mjs",
    "getlightningsendrequest": "node utexo_test/getlightningsendrequest.mjs",
    "getlightningreceiverequest": "node utexo_test/getlightningreceiverequest.mjs"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@bitcoindevkit/bdk-wallet-node": "^0.2.0",
    "@bitcoindevkit/bdk-wallet-web": "^0.2.0",
    "@bitcoinerlab/secp256k1": "^1.2.0",
    "@noble/hashes": "^2.0.1",
    "axios": "^1.8.4",
    "bare-node-runtime": "^1.1.4",
    "bip32": "^5.0.0-rc.0",
    "bip39": "^3.1.0",
    "bitcoinjs-lib": "^6.1.7",
    "form-data": "^4.0.0",
    "@utexo/rgb-lib": "0.3.0-beta.8",
    "ripemd160": "^2.0.3"
  },
  "optionalDependencies": {
    "@utexo/rgb-lib-linux-x64": "0.3.0-beta.8",
    "@utexo/rgb-lib-linux-arm64": "0.3.0-beta.8",
    "@utexo/rgb-lib-darwin-arm64": "0.3.0-beta.8"
  },
  "keywords": [
    "rgb",
    "rgb-protocol",
    "bitcoin",
    "sdk",
    "bdk",
    "wasm",
    "taproot",
